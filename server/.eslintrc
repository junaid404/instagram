{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": "off",
    "eslint-disable-next-line": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": "off"
  }
}
